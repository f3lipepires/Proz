for andar in range(20, 0, -1):
    if andar != 13:
        print(andar)

andar = 20
while andar >= 1:
    if andar != 13:
        print(andar)
    andar -= 1

andares = list(range(20, 0, -1))
andares.remove(13)
for andar in andares:
    print(andar)
